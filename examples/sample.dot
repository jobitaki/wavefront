digraph dataflow {
    rankdir=LR;
    node [shape=box, style=filled, fillcolor=lightblue];
    
    // Nodes represent instructions
    LOAD_A [label="LOAD A"];
    LOAD_B [label="LOAD B"];
    ADD [label="ADD"];
    MUL [label="MUL"];
    STORE [label="STORE"];
    
    // Edges represent data dependencies
    LOAD_A -> ADD [label="operand1"];
    LOAD_B -> ADD [label="operand2"];
    LOAD_A -> MUL [label="operand1"];
    ADD -> MUL [label="operand2"];
    MUL -> STORE [label="result"];
}
